{
  "name": "webpack5-vue3-starter",
  "version": "1.0.0",
  "description": "a vue3 project template based on webpack5",
  "packageManager": "pnpm@7.5.0",
  "private": true,
  "author": {
    "name": "azixf",
    "email": "xie0729@yeah.net"
  },
  "homepage": "https://github.com/azixf/webpack5-vue3-starter#readme",
  "bugs": "https://github.com/azixf/webpack5-vue3-starter/issue",
  "repository": {
    "type": "git",
    "url": "https://github.com/azixf/webpack5-vue3-starter"
  },
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config ./build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.js",
    "analyze": "webpack-bundle-analyzer --port 3000 ./dist/stats.json",
    "lint-types": "vue-tsc --noEmit",
    "prepare": "husky install",
    "commit": "git-cz",
    "eslint": "eslint --fix --ext .js,.ts,.vue,.tsx,.jsx",
    "prettier": "prettier --write ./src/**/*.{ts,js,vue}",
    "lint-staged": "npx lint-staged"
  },
  "keywords": [
    "webpack5",
    "vue3",
    "template"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@babel/runtime-corejs3": "^7.18.6",
    "@squoosh/lib": "^0.4.0",
    "@types/node": "^18.0.4",
    "@types/nprogress": "^0.2.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vue/compiler-sfc": "^3.2.37",
    "ant-design-vue": "^3.2.10",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.4",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "3.23.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.16",
    "postcss": "8.4.6",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.7.4",
    "uglify-js": "^3.16.2",
    "unplugin-auto-import": "^0.9.3",
    "unplugin-vue-components": "^0.21.1",
    "vue": "^3.2.36",
    "vue-eslint-parser": "^9.0.3",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.1.2",
    "vue-tsc": "^0.38.9",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz.config.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue,jsx,tsx}": [
      "eslint --fix --ext .js,.ts,.vue,.tsx,.jsx",
      "prettier --write"
    ]
  }
}
